<!DOCTYPE html>
<html>
<head>
    <title>Data Display</title>
</head>
<body>
    <div class="data-box">
        <h2>Exam ID: <%= userBB.ExamID %></h2>
        <h2>Selected Faculty Data</h2>
        <table>
            <tr>
                <th>Faculty Name</th>
                <th>Faculty ID</th>
                <th>Course Code</th>
                <th>Course Title</th>
                <th>Semester</th>
            </tr>
            <% userBB.forEach(Faculty => { %>
            <tr>
                <td><%= Faculty.Fname %></td>
                <td><%= Faculty.Fid %></td>
                <td><%= Faculty.Ccode %></td>
                <td><%= Faculty.Ctitle %></td>
                <td><%= Faculty.Semester %></td>
            </tr>
            <% }); %>
        </table>
    </div>

    <div class="data-box">
        <h2>Student Data</h2>
        <table>
            <tr>
                <th>Student Name</th>
                <th>Student ID</th>
                <th>Marks</th>
                <th>CO 1</th>
                <th>CO 2</th>
                <th>CO 3</th>
                <th>CO 4</th>
                <th>CO 5</th>
                <th>CO 6</th>
                <th>CO 7</th>
            </tr>
            <% userBB.forEach(student => { %>
                <% student.studentName.forEach((name, index) => { %>
                    <tr>
                        <td><%= name %></td>
                        <td><%= student.studentIDvalue[index] %></td>
                        <td><%= student.Marks[index] %></td>
                        <td><%= student.co1[index] %></td>
                        <td><%= student.co2[index] %></td>
                        <td><%= student.co3[index] %></td>
                        <td><%= student.co4[index] %></td>
                        <td><%= student.co5[index] %></td>
                        <td><%= student.co6[index] %></td>
                        <td><%= student.co7[index] %></td>
                    </tr>
                <% }); %>
            <% }); %>
        </table>
    </div>
</body>
</html>
